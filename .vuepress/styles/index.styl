  .explanation {
    padding: 16px;
    border: 1px solid rgba(231, 237, 253, 1);
    border-left: 4px solid #2F54EB;
    background-color: #F5F8FF;
    margin: 24px 0;
    color: #666666;
  }

  .explanation-icon {
    padding: 0 4px;
  }

  ul li::marker {
    color: #2F54EB;
  }


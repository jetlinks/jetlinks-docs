  .explanation-title-warp {
    margin-top:-2px;
  }
  .primary .explanation-icon, .primary .explanation-title {
    color: #2F54EB;
    margin: 0 0 10px 0;
  }

  .warning {
    background-color: rgba(250, 178, 71, 0.1);
    border-color: rgba(250, 178, 71, 0.4);
  }

  .warning .explanation-icon, .warning .explanation-title {
    color: #FBAC37;
  }

  .error {
    background-color: rgba(247, 111, 93, 0.1);
    border-color: rgba(247, 111, 93, 0.4);
  }

  .error .explanation-icon, .error .explanation-title {
    color: #F76F5D;
  }

  .info {
    background-color: rgba(66, 147, 255, 0.1);
    border-color: rgba(66, 147, 255, 0.4);
  }

  .info .explanation-icon, .info .explanation-title {
    color: #4293FF;
  }

  .info ul li::marker {
    color: #4293FF;
  }

  .collapse {
    border: 1px solid #F0F0F0 !important;
    margin: 16px 0 !important;
  }

  .collapse-title {
    background: #FAFAFA !important;
    padding: 9px 18px !important;
  }

  .collapse-body {
    padding: 16px !important;
  }

  .no-margin {
    margin: 0 !important;
  }

  .table {
    width: 100% !important;
    display: table;
  }

  .table td {
    border-color: #F0F0F0 !important;
     word-break: break-all !important;
     min-width:50px
  }

  .table tbody tr:nth-child(even) {
    background-color: #F6F8FA !important;
  }

  .divider {
    box-sizing: border-box !important;
    margin: 0 !important;
    padding: 0 !important;
    color: #000000d9 !important;
    font-size: 14px !important;
    font-variant: tabular-nums !important;
    line-height: 1.5715 !important;
    list-style: none !important;
    font-feature-settings: "tnum" !important;
    border-top: 1px solid rgba(0,0,0,.06) !important;
    margin: 16px 0 !important;
  }

  img {
    vertical-align: text-bottom !important;
  }

  p {
    margin-block-start: .6em;
    margin-block-end: .6em;
  }

  .no-border {
    border: none;
  }

